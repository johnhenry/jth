// Basic arithmetic operations

2 3 + @;    // 5
10 3 - @;   // 7
4 5 * @;    // 20
15 4 / @;   // 3.75
17 5 % @;   // 2
2 8 ** @;   // 256

// Chained operations
2 3 + 4 * @;   // (2 + 3) * 4 = 20

// Increment and decrement
10 ++ @;   // 11
10 -- @;   // 9

// Other math
-7 abs @;     // 7
16 sqrt @;    // 4
3.7 floor @;  // 3
3.2 ceil @;   // 4
