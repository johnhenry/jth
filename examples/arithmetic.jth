// Basic arithmetic operations

2 3 + peek;    // 5
10 3 - peek;   // 7
4 5 * peek;    // 20
15 4 / peek;   // 3.75
17 5 % peek;   // 2
2 8 ** peek;   // 256

// Chained operations
2 3 + 4 * peek;   // (2 + 3) * 4 = 20

// Increment and decrement
10 ++ peek;   // 11
10 -- peek;   // 9

// Other math
-7 abs peek;     // 7
16 sqrt peek;    // 4
3.7 floor peek;  // 3
3.2 ceil peek;   // 4
