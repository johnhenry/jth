{
  "name": "jth-core",
  "version": "0.2.0",
  "description": "jth is an alternative syntax for javascript centered around sequential data processing.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnhenry/jth.git"
  },
  "exports": {
    ".": "./index.mjs",
    "./context": "./context.mjs",
    "./runtime": "./runtime.mjs",
    "./static-runtime": "./static-runtime.mjs",
    "./process-line": "./process-line.mjs",
    "./js-line": "./js-line.mjs",
    "./transform": "./transform.mjs",
    "./core": "./core.mjs",
    "./process-n": "./process-n.mjs",
    "./runtime/extract-global": "./external/extract-global.mjs"
  },
  "scripts": {
    "test": "node --test test/*.test.mjs",
    "test:watch": "node --test --watch test/*.test.mjs",
    "test:all": "node test/run-all.mjs",
    "test:core": "node --test test/core.test.mjs",
    "test:math": "node --test test/math.test.mjs",
    "test:data": "node --test test/data-structures.test.mjs",
    "test:stats": "node --test test/statistics.test.mjs",
    "test:new": "node --test test/new-features.test.mjs",
    "link": "exit 0",
    "unlink": "exit 0",
    "build-static-runtime": "rollup ./runtime.mjs -o ./static-runtime.mjs -f iife",
    "publish": "npm run build-static-runtime -s && npm publish"
  },
  "main": "./index.mjs",
  "dependencies": {
    "split-string": "^6.1.0"
  },
  "devDependencies": {
    "pop-quiz": "^0.0.5",
    "rollup": "^2.79.0"
  },
  "keywords": [
    "jth",
    "stack-oriented",
    "concatenative",
    "functional",
    "programming-language",
    "data-processing",
    "javascript",
    "compiler",
    "stack-based",
    "forth-like"
  ],
  "homepage": "https://github.com/johnhenry/jth#readme",
  "bugs": {
    "url": "https://github.com/johnhenry/jth/issues"
  },
  "author": "John Henry",
  "license": "ISC"
}